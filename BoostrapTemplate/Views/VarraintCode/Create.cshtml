@model OMS_Template.ViewModels.VarraintCode.VarraintCode_Add_ViewModel
@{
    Layout = null;
}


<form asp-action="Create" enctype="multipart/form-data" class="row g-3">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="col-md-6">
        <input asp-for="Erp_Vcode" type="text" class="form-control" placeholder="Variant Code">
        <span asp-validation-for="Erp_Vcode" class="text-danger"></span>
    </div> 
    <div class="col-md-6">
        <input asp-for="Mes_Vcode" type="number" class="form-control" placeholder="Mes Vcode">
        <span asp-validation-for="Mes_Vcode" class="text-danger"></span>
    </div>
    <div class="col-md-12">
        <input asp-for="Description" type="text" class="form-control" placeholder="Description">
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
   

    <div class="col-md-12 modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <input type="submit" class="btn btn-sm btn-info p-2" style="float:right;" value="Submit" id="btt" />
    </div>



    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>


</form><!-- End No Labels Form -->
